{
  "name": "chessahoochee",
  "version": "0.1.0",
  "description": "A chess tournament manager",
  "author": "John Jackson",
  "license": "GPL-3.0-or-later",
  "private": true,
  "homepage": ".",
  "main": "public/electron.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/johnridesabike/chessahoochee.git"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "coverage": "npm run test -- --coverage",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "sourcemaps": "source-map-explorer './build/static/js/*'",
    "lint": "eslint --ext .jsx,.js src/",
    "electron-dev": "concurrently \"BROWSER=none npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-dev-win": "concurrently \"SET BROWSER=none\" \"npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "postinstall": "electron-builder install-app-deps",
    "preelectron-pack": "npm run build",
    "electron-pack": "electron-builder build -mw"
  },
  "dependencies": {
    "@reach/dialog": "^0.2.8",
    "@reach/router": "^1.2.1",
    "@reach/tabs": "^0.1.5",
    "@reach/tooltip": "^0.2.0",
    "@reach/visually-hidden": "^0.1.4",
    "dashify": "^2.0.0",
    "edmonds-blossom": "^1.0.0",
    "electron-is-dev": "^1.1.0",
    "elo-rank": "^1.0.3",
    "hash-source": "^1.0.4",
    "numeral": "^2.0.6",
    "prop-types": "^15.7.2",
    "ramda": "^0.26.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-feather": "^1.1.6",
    "tcomb": "^3.2.29",
    "thenby": "^1.3.0"
  },
  "devDependencies": {
    "jest-dom": "^3.2.2",
    "react-scripts": "3.0.1",
    "react-testing-library": "^7.0.1",
    "concurrently": "^4.1.0",
    "electron": "^5.0.1",
    "electron-builder": "^20.40.2",
    "electron-devtools-installer": "^2.2.4",
    "eslint-plugin-fp": "^2.3.0",
    "eslint-plugin-ramda": "^2.5.1",
    "eslint-plugin-react": "^7.13.0",
    "gh-pages": "^2.0.1",
    "react-axe": "^3.1.0",
    "typescript": "^3.4.5",
    "wait-on": "^3.2.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "build": {
    "appId": "bike.johnridesa.chessahoochee",
    "productName": "Chessahoochee",
    "copyright": "Copyright Â© 2019 ${author}",
    "mac": {
      "category": "public.app-category.productivity"
    },
    "directories": {
      "buildResources": "assets"
    }
  }
}
