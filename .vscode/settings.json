{
    "editor.detectIndentation": false,
    "javascript.validate.enable": false,
    "typescript.suggest.enabled": false,
    "typescript.suggestionActions.enabled": false,
    "javascript.suggestionActions.enabled": false,
    "jslint.version": "es6",
    "jslint.exclude": {
        "**/*.jsx.*": true,
        "**/node_modules/*": true,
        "**/build/**": true,
        "**/flow-typed/**": true,
        "**/flow-types.js": true
    },
    "jslint.options": {
        "devel": true,
        "predef": ["it", "expect"],
        "this": false
    }
}