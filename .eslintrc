{
    "extends": ["react-app", "plugin:jsx-a11y/recommended"],
    "plugins": ["jsx-a11y"],
    "rules": {
        "jsx-a11y/label-has-for": "off",
        "max-len": [
            "error", {
                "code": 80,
                "ignoreComments": true
            }
        ],
        "no-var": 1,
        "semi": "warn",
        "arrow-parens": "warn",
        "space-infix-ops": "warn",
        "no-trailing-spaces": "warn"
    }
}